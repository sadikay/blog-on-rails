<%= form_for [:admin, @images], data: {abide: ''} do |form| %>    <div>      <br>      <div>        <%= form.label "Image Title:" %>        <%= form.text_area :title, required: true %>      </div>      <br>      <div>        <%= form.label "Image:" %>        <%= form.file_field :image %>      </div>      <br>      <div>        <%= form.submit %>      </div>    </div><% end %>