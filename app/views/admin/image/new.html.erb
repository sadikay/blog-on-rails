<%= form_for [:admin, @images], data: {abide: ''} do |form| %>    <% if @images.errors.any? %>        <div class="row">          <p>Incorrect values</p>          <ul class="square">            <% @images.errors.full_messages.each do |message| %>                <li><%= message %></li>            <% end %>          </ul>        </div>    <% end %>    <br><br>    <div>      <br>      <div>        <%= form.label "Image Title:" %>        <%= form.text_area :title, required: true %>      </div>      <br>      <div>        <%= form.label "Image:" %>        <%= form.file_field :image %>      </div>      <br>      <div>        <%= form.submit %>      </div>    </div><% end %>